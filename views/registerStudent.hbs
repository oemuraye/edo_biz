<script>


   let objpara2 = "";
 window.onload = function () {
  var url = document.location.href,
      params = url.split('?')[1].split('&'),
      data = {}, tmp;
  for (var i = 0, l = params.length; i < l; i++) {
       tmp = params[i].split('=');
       data[tmp[0]] = tmp[1];
  }

let para = decodeURI(data.info)

let objpara = JSON.parse(para)
  document.getElementById('title1').innerText = objpara;
  objpara2 = objpara;
}

    $(document).ready(function(){
       $("#btn1").submit(function(e){
        e.preventDefault();

       

        
       let formValue = new FormData(e.target);
       const formProps = Object.fromEntries(formValue);
       
   if(formProps.address === ""||
    formProps.country===""||
    formProps.email ===""||
    formProps.state ===""||
    formProps.name ===""||
    formProps.phone ===""
    ){
    showToast('Field Cannot be Empty');
   }else{
   

    let button1 = document.getElementById("button1");
        let loader = document.getElementById('loader')
        button1.style.display = 'none';
        loader.style.display = "block";
    // loader.style.display = "block";
fetch("https://brykiva.herokuapp.com/api/order", {
  method: "POST",
  headers: {'Content-Type': 'application/json'}, 
  body: JSON.stringify({...formProps, title:objpara2})
}).then(res => {
    loader.style.display = "none";
  console.log("Request complete! response:", res);
  button1.style.display = 'block';
  loader.style.display = "none";

  let card = document.getElementById("card")
  let login = document.getElementById("login-root")
  login.style.display = "none";
    card.style.display = "block";
});
   }
       
       });

    
    });
    
    </script>

